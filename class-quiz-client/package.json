{
  "name": "class-quiz-client",
  "version": "1.0.0",
  "description": "CS 597",
  "repository": "http://github.com/ryanthompson0123/class-quiz.git",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --progress --inline --hot > class-quiz-client.log 2>&1 &",
    "webpack": "webpack",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js 'test/**/*.@(js|jsx)'",
    "test:watch": "npm run test -- --watch",
    "lint-all": "eslint '**/*.js' '**/*.jsx' "
  },
  "keywords": [],
  "author": "",
  "license": "PROPRIETARY",
  "devDependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.1",
    "babel-plugin-extensible-destructuring": "^3.0.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.5.0",
    "babel-root-import": "^3.1.0",
    "extensible-polyfill": "^1.0.1",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.3",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.16.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "imports-loader": "^0.6.5",
    "jsdom": "^8.0.2",
    "mocha": "^2.4.5",
    "postcss-loader": "^0.8.0",
    "react-transform-hmr": "^1.0.2",
    "redux-devtools": "^3.0.2",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "style-loader": "^0.13.0",
    "typescript": "^1.9.0-dev.20160209",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-1"
    ],
    "env": {
      "development": {
        "plugins": [
          [
            "react-transform",
            {
              "transforms": [
                {
                  "transform": "react-transform-hmr",
                  "imports": [
                    "react"
                  ],
                  "locals": [
                    "module"
                  ]
                }
              ]
            }
          ],
          [
              "extensible-destructuring",
              {
                "mode": "optout"
              }
            ],
          "transform-decorators-legacy",
          "babel-root-import"
        ]
      },
      "production": {
        "plugins": [
          "transform-decorators-legacy",
          [
            "extensible-destructuring",
            {
              "mode": "optout"
            }
          ],
          "babel-root-import"
        ]
      }
    }
  },
  "dependencies": {
    "autobind-decorator": "^1.3.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.3.14",
    "classnames": "^2.2.3",
    "flexboxgrid": "^6.3.0",
    "immutable": "^3.7.6",
    "material-ui": "^0.14.3",
    "moment": "^2.11.2",
    "node-sass": "^3.4.2",
    "react": "^0.14.7",
    "react-addons-pure-render-mixin": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-flexbox-grid": "^0.9.4",
    "react-immutable-proptypes": "^1.5.1",
    "react-redux": "^4.2.1",
    "react-tap-event-plugin": "^0.2.2",
    "redux": "^3.2.0",
    "redux-logger": "^2.4.0",
    "redux-thunk": "^1.0.3",
    "sass-loader": "^3.1.2",
    "socket.io-client": "1.4.5",
    "whatwg-fetch": "^0.11.0"
  }
}